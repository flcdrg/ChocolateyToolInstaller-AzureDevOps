{
    "manifestVersion": 1,
    "id": "chocolatey-tool-installer-task",
    "name": "Chocolatey Installer",
    "version": "1.0.4",
    "publisher": "DavidGardiner",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Installs Chocolatey",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/icon.png"        
    },
    "files": [
        {
            "path": "installerTask"
        }
    ],
    "contributions": [
        {
            "id": "chocolatey-tool-installer-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "installerTask"
            }
        }
    ],
    "tags": [
        "chocolatey",
        "installer"
    ],
    "links": {
        "license": {
            "uri": "https://github.com/flcdrg/ChocolateyToolInstaller-AzureDevOps/blob/master/LICENSE"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/flcdrg/ChocolateyToolInstaller-AzureDevOps"
      }
    }
}